{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "dev": {
            "dependsOn": [
                "^dev"
            ],
            "cache": false,
            "persistent": true
        },
        "build": {
            "dependsOn": [
                "^build"
            ],
            "cache": true
        },
        "test": {
            "dependsOn": [
                "^test"
            ],
            "cache": false
        }
    }
}